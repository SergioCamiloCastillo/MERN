Success(inputData);\n  const resultValidation = emailValid.test(value);\n  if (resultValidation) {\n    inputData.classList.add(\"success\");\n    return true;\n  } else {\n    inputData.classList.add(\"error\");\n  }\n}\nfunction removeClassErrorSuccess(inputData) {\n  inputData.classList.remove(\"success\");\n  inputData.classList.remove(\"error\");\n}\n","/home/sergio/blog-MERN/client/src/api/user.js",[],"/home/sergio/blog-MERN/client/src/api/config.js",[],"/home/sergio/blog-MERN/client/src/components/Admin/LoginForm/index.js",[],"/home/sergio/blog-MERN/client/src/components/Admin/LoginForm/LoginForm.js",[],"/home/sergio/blog-MERN/client/src/utils/constants.js",[],"/home/sergio/blog-MERN/client/src/api/auth.js",["131"],"import { basePath, apiVersion } from \"./config\";\nimport { ACCESS_TOKEN, REFRESH_TOKEN } from \"../utils/constants\";\nimport jwtDecode from \"jwt-decode\";\n\nexport function getAccessTokenApi() {\n  const accessToken = localStorage.getItem(ACCESS_TOKEN);\n\n  if (!accessToken || accessToken === \"null\") {\n    return null;\n  }\n\n  return willExpireToken(accessToken) ? null : accessToken;\n}\nexport function refreshAccessTokenApi(refreshToken) {\n  const url = `${basePath}/${apiVersion}/refresh-access-token`;\n  const bodyObj = {\n    refreshToken: refreshToken,\n  };\n  const params = {\n    method: \"POST\",\n    body: JSON.stringify(bodyObj),\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n  };\n  fetch(url, params)\n    .then((response) => {\n      if (response != 200) {\n        return null;\n      }\n      return response.JSON;\n    })\n    .then((result) => {\n      if (!result) {\n        logout();\n      } else {\n        const { accessToken, refreshToken } = result;\n        localStorage.setItem(ACCESS_TOKEN, accessToken);\n        localStorage.setItem(REFRESH_TOKEN, refreshToken);\n      }\n    });\n}\nexport function getRefreshTokenApi() {\n  const refreshToken = localStorage.getItem(REFRESH_TOKEN);\n  if (!refreshToken || refreshToken === \"null\") {\n    return null;\n  }\n  return willExpireToken(refreshToken) ? null : refreshToken;\n}\nexport function logout() {\n  localStorage.removeItem(ACCESS_TOKEN);\n  localStorage.removeItem(REFRESH_TOKEN);\n}\nfunction willExpireToken(token) {\n  const seconds = 60;\n  const metaToken = jwtDecode(token);\n  const { exp } = metaToken;\n  const now = (Date.now() + seconds) / 1000;\n  return now > exp;\n}\n","/home/sergio/blog-MERN/client/src/provider/AuthProvider.js",["132","133"],"/home/sergio/blog-MERN/client/src/hooks/useAuth.js",["134"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../provider/AuthProvider\";\nexport default () => useContext(AuthContext);\n",{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","severity":1,"message":"140","line":2,"column":8,"nodeType":"141","messageId":"142","endLine":2,"endColumn":13},{"ruleId":"139","severity":1,"message":"143","line":9,"column":10,"nodeType":"141","messageId":"142","endLine":9,"endColumn":27},{"ruleId":"139","severity":1,"message":"144","line":9,"column":29,"nodeType":"141","messageId":"142","endLine":9,"endColumn":47},{"ruleId":"145","severity":1,"message":"146","line":64,"column":3,"nodeType":"147","messageId":"148","endLine":64,"endColumn":10},{"ruleId":"139","severity":1,"message":"143","line":4,"column":10,"nodeType":"141","messageId":"142","endLine":4,"endColumn":27},{"ruleId":"149","severity":1,"message":"150","line":13,"column":36,"nodeType":"151","messageId":"152","endLine":13,"endColumn":37,"suggestions":"153"},{"ruleId":"149","severity":1,"message":"154","line":13,"column":38,"nodeType":"151","messageId":"152","endLine":13,"endColumn":39,"suggestions":"155"},{"ruleId":"149","severity":1,"message":"156","line":13,"column":43,"nodeType":"151","messageId":"152","endLine":13,"endColumn":44,"suggestions":"157"},{"ruleId":"149","severity":1,"message":"154","line":13,"column":57,"nodeType":"151","messageId