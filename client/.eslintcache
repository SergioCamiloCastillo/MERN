  \"Content-Type\": \"application/json\",\n    },\n  };\n  fetch(url, params)\n    .then((response) => {\n      if (response != 200) {\n        return null;\n      }\n      return response.JSON;\n    })\n    .then((result) => {\n      if (!result) {\n        logout();\n      } else {\n        const { accessToken, refreshToken } = result;\n        localStorage.setItem(ACCESS_TOKEN, accessToken);\n        localStorage.setItem(REFRESH_TOKEN, refreshToken);\n      }\n    });\n}\nexport function getRefreshTokenApi() {\n  const refreshToken = localStorage.getItem(REFRESH_TOKEN);\n  if (!refreshToken || refreshToken === \"null\") {\n    return null;\n  }\n  return willExpireToken(refreshToken) ? null : refreshToken;\n}\nexport function logout() {\n  localStorage.removeItem(ACCESS_TOKEN);\n  localStorage.removeItem(REFRESH_TOKEN);\n}\nfunction willExpireToken(token) {\n  const seconds = 60;\n  const metaToken = jwtDecode(token);\n  const { exp } = metaToken;\n  const now = (Date.now() + seconds) / 1000;\n  return now > exp;\n}\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/SignIn/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/SignIn/SignIn.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/Admin.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/hooks/useAuth.js",["159"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../provider/AuthProvider\";\nexport default () => useContext(AuthContext);\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/Users/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/api/config.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/utils/constants.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/Users/Users.jsx",[],"/Users/desarrollo