classList.add(\"error\");\n  }\n}\nfunction removeClassErrorSuccess(inputData) {\n  inputData.classList.remove(\"success\");\n  inputData.classList.remove(\"error\");\n}\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Modal/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Modal/Modal.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Users/ListUsers/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Users/ListUsers/ListUsers.jsx",["170","171","172"],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Users/EditUserForm/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/components/Admin/Users/EditUserForm/EditUserForm.jsx",["173","174"],"import React, { useCallback, useState, useEffect } from 'react';\nimport \"./EditUserForm.scss\";\nimport {\n    UserOutlined, MailOutlined, LockOutlined, EyeInvisibleOutlined,\n    EyeTwoTone,\n\n} from \"@ant-design/icons\";\nimport { Avatar, Form, Input, Select, Button, Col, Row } from \"antd\";\nimport { useDropzone } from 'react-dropzone';\nimport NoAvatar from \"../../../../assets/img/png/no-avatar.png\";\nimport { getAvatarApi } from \"../../../../api/user\";\nexport default function EditUserForm(props) {\n    const { user } = props;\n    const [avatar, setAvatar] = useState(null);\n    const [userData, setUserData] = useState({});\n\n    useEffect(() => {\n        setUserData({\n            name: user.name,\n            lastname: user.lastname,\n            email: user.email,\n            role: user.role,\n            avatar: user.avatar\n        })\n    }, [user]);\n    useEffect(() => {\n        if (user.avatar) {\n            getAvatarApi(user.avatar).then(response => {\n                setAvatar(response);\n            });\n        } else {\n            setAvatar(null);\n        }\n    }, [user]);\n    useEffect(() => {\n     