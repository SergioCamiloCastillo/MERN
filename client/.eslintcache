} />\n        <Layout\n          className=\"layout-admin\"\n          style={{ marginLeft: menuCollapsed ? \"80px\" : \"200px\" }}\n        >\n          <Header className=\"layout-admin__header\">\n            \n            <MenuTop\n              menuCollapsed={menuCollapsed}\n              setMenuCollapse={setMenuCollapse}\n            />\n          </Header>\n          <Content className=\"layout-admin__content\">\n            <LoadRoutes routes={routes}></LoadRoutes>\n          </Content>\n          <Footer className=\"layout-admin__footer\">Sergio Castillo</Footer>\n        </Layout>\n      </Layout>\n    );\n  }else{\n    return null;\n  }\n}\n\nfunction LoadRoutes({ routes }) {\n  return (\n    <Switch>\n      {routes.map((route, index) => (\n        <Route\n          key={index}\n          path={route.path}\n          exact={route.exact}\n          component={route.component}\n        ></Route>\n      ))}\n    </Switch>\n  );\n  return;\n}\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Error404.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Home.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Contact.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/layouts/LayoutBasic.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/api/auth.js",["261"],"import { basePath, apiVersion } from \"./config\";\nimport { ACCESS_TOKEN, REFRESH_TOKEN } from \"../utils/constants\";\nimport jwtDecode from \"jwt-decode\";\n\nexport function getAccessTokenApi() {\n  const accessToken = localStorage.getItem(ACCESS_TOKEN);\n\n  if (!accessToken || accessToken === \"null\") {\n    return null;\n  }\n\n  return willExpireToken(accessToken) ? null : accessToken;\n}\nexport function refreshAccessTokenApi(refreshToken) {\n  const url = `${basePath}/${apiVersion}/refresh-access-token`;\n  const bodyObj = {\n    refreshToken: refreshToken,\n  };\n  const params = {\n    method: \"POST\",\n    body: JSON.stringify(bodyObj),\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n  };\n  fetch(url, params)\n    .then((response) => {\n      if (response != 200) {\n        return null;\n      }\n      return response.JSON;\n    })\n    .then((result) => {\n      if (!result) {\n        logout();\n      } else {\n        const { accessToken, refreshToken } = result;\n        localStorage.setItem(ACCESS_TOKEN, accessToken);\n        localStorage.setItem(REFRESH_TOKEN, refreshToken);\n      }\n    });\n}\nexport function getRefreshTokenApi() {\n  const refreshToken = localStorage.getItem(REFRESH_TOKEN);\n  if (!refreshToken || refreshToken === \"null\") {\n    return null;\n  }\n  return willExpireToken(refreshToken) ? null : refreshToken;\n}\nexport function logout() {\n  localStorage.removeItem(ACCESS_TOKEN);\n  localStorage.removeItem(REFRESH_TOKEN);\n}\nfunction willExpireToken(token) {\n  const seconds = 60;\n  const metaToken = jwtDecode(token);\n  const { exp } = metaToken;\n  const now = (Date.now() + seconds) / 1000;\n  return now > exp;\n}\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/SignIn/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/SignIn/SignIn.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/Admin.jsx",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/hooks/useAuth.js",["262"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../provider/AuthProvider\";\nexport default () => useContext(AuthContext);\n","/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/pages/Admin/Users/index.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/api/config.js",[],"/Users/desarrollo5omega/Documents/proyectos/MERN/client/src/utils/constants.js",[],"/Users/desarrollo5omega/Documents/proyectos/